#include <stdio.h>
int main() {
    float num1, num2;
    printf("digite o primeiro numero: ");
    scanf("%f", &num1);
    printf("digite o segundo numero: ");
    scanf("%f", &num2);
    if(num1>num2){
        printf("o primeiro numero (%.2f) é o maior",num1);
    }
    else{
        printf("o segundo numero (%.2f) é o maior",num2);
    }
    
    return 0;
}